# Example .env for qBittorrent Torrent File Whitelist

QB_URL="http://localhost:8080"
QB_USER="user"
QB_PASS="changeme"
LOGFILE="/tmp/on-torrent-added.log"
COOKIE_JAR="/tmp/qbt-cookies.txt"

# Only files with these extensions are allowed (case-insensitive, pipe-separated)
ALLOWED_EXTENSIONS="mp4|mkv|avi|mov|wmv|flv|webm|mpeg|mpg|ts|m4v"

# Categories this filter should apply to (pipe-separated, lowercase)
FILTER_CATEGORIES="tv|movies"
